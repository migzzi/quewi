{
  "name": "quewi",
  "version": "1.0.0",
  "description": "quewi is a simple, fast, and easy to use queue system for Node.js",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "rm -rf dist && npx tsc -p tsconfig.build.json",
    "format": "prettier --write \"lib/**/*.ts\" \"test/**/*.ts\"",
    "lint": "eslint \"{lib,test}/**/*.ts\"",
    "lint-fix": "eslint \"{lib,test}/**/*.ts\" --fix",
    "test": "jest --config ./jest.json",
    "test:e2e": "jest --config ./jest-e2e.json --runInBand",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/migzzi/quewi.git"
  },
  "keywords": [
    "queue",
    "fast",
    "caching"
  ],
  "author": "magedmagdy105@gmail.com",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Mahmoud-Awaed/LIB_SuperCache/issues"
  },
  "homepage": "https://github.com/Mahmoud-Awaed/LIB_SuperCache#readme",
  "devDependencies": {
    "@stylistic/eslint-plugin": "^2.1.0",
    "@types/jest": "29.2.4",
    "@types/luxon": "^3.4.2",
    "@types/node": "18.11.18",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@wcj/html-to-markdown": "^2.1.0",
    "benny": "^3.7.1",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.3.1",
    "jest-sonar-reporter": "^2.0.0",
    "prettier": "^2.3.2",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "4.1.1",
    "typescript": "^4.9.5"
  },
  "files": [
    "dist",
    "README.md"
  ]
}
